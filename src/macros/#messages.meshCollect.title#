if !move.axes[0].homed					; conditional X homing
	M1291 C3 T6 D0
	M291 R"#messages.meshCollect.title#" P"#messages.home#X#" S0 T90
	G28 X
if !move.axes[1].homed					; conditional Y homing
	M1291 C3 T6 D0
	M291 R"#messages.meshCollect.title#" P"#messages.home#Y#" S0 T90
	G28 Y

T0 P0															; select left tool
M98 P"/sys/machine-specific/head0-down.g"
;G28 Z								;standard Z homing
M1291 C5 T6 D0 
